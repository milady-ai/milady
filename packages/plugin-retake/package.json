{
  "name": "@milady/plugin-retake",
  "description": "Retake.tv RTMP streaming destination plugin with chat integration for ElizaOS agents",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageType": "plugin",
  "platform": "node",
  "license": "MIT",
  "keywords": ["elizaos", "elizaos-plugin", "retake", "streaming", "rtmp", "chat"],
  "repository": {
    "type": "git",
    "url": "https://github.com/milady-ai/milady.git",
    "directory": "packages/plugin-retake"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "bun": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "dependencies": {
    "@elizaos/core": "next"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "bun run build.ts",
    "test": "bun test"
  },
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "RETAKE_AGENT_TOKEN": {
        "type": "string",
        "description": "Retake.tv agent access token for API authentication",
        "required": true
      },
      "RETAKE_API_URL": {
        "type": "string",
        "description": "Retake.tv API base URL (defaults to https://retake.tv/api/v1)",
        "required": false
      },
      "RETAKE_USER_DB_ID": {
        "type": "string",
        "description": "Retake.tv user database ID (auto-discovered if not set)",
        "required": false
      }
    }
  }
}
