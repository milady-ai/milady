{
  "name": "@milady/plugin-twitch-streaming",
  "description": "Twitch RTMP streaming destination plugin for ElizaOS agents",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageType": "plugin",
  "platform": "node",
  "license": "MIT",
  "keywords": ["elizaos", "elizaos-plugin", "twitch", "streaming", "rtmp"],
  "repository": {
    "type": "git",
    "url": "https://github.com/milady-ai/milady.git",
    "directory": "packages/plugin-twitch"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "bun": "./src/index.ts",
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "package.json"
  ],
  "dependencies": {
    "@elizaos/core": "next"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  },
  "scripts": {
    "build": "bun run build.ts",
    "test": "bun test"
  },
  "agentConfig": {
    "pluginType": "elizaos:plugin:1.0.0",
    "pluginParameters": {
      "TWITCH_STREAM_KEY": {
        "type": "string",
        "description": "Twitch stream key for RTMP ingest",
        "required": true
      }
    }
  }
}
