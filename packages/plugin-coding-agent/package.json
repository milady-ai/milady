{
  "name": "@milaidy/plugin-coding-agent",
  "version": "0.1.0",
  "description": "Coding agent orchestration plugin - spawn and manage CLI coding agents via PTY",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "bun run build.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "@elizaos/core": ">=2.0.0-alpha.0",
    "git-workspace-service": ">=0.3.0",
    "pty-manager": ">=1.8.0"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "typescript": "^5.9.3"
  },
  "license": "MIT",
  "dependencies": {
    "coding-agent-adapters": "0.8.0",
    "pty-console": "0.3.0"
  }
}
